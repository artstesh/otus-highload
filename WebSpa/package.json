{
  "name": "schematics",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "format": "prettier --write \"src/**/*.(ts|html|scss)\"",
    "hsky": "cd ../../ && husky install ./WebSpa/Weather/.husky",
    "pretty-quick": "pretty-quick --staged",
    "lint": "ng lint --fix=true",
    "build": "ng build --configuration development",
    "build:prod": "ng build",
    "build:staging": "ng build --configuration staging",
    "build:predprod": "ng build --configuration predprod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "openapi-gen-fields-develop": "ng-openapi-gen -c src/environments/openapi/fields/develop.json",
    "openapi-gen-geo-develop": "ng-openapi-gen -c src/environments/openapi/geo/develop.json",
    "i18n:extract": "ngx-translate-extract --input ./src --output src/assets/i18n/en-us.json src/assets/i18n/uz-latn.json src/assets/i18n/ru-ru.json --sort --format json",
    "compodoc:build": "compodoc -p tsconfig.doc.json -t",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s"
  },
  "author": "6thGrain",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "17.3.9",
    "@angular-devkit/schematics": "17.3.9",
    "@angular/cdk": "17.3.10",
    "@angular/common": "17.3.12",
    "@angular/compiler": "17.3.12",
    "@angular/core": "17.3.12",
    "@angular/forms": "17.3.12",
    "@angular/platform-browser": "17.3.12",
    "@angular/platform-browser-dynamic": "17.3.12",
    "@angular/router": "17.3.12",
    "@artstesh/maps": "6.1.6",
    "@artstesh/modals": "6.1.0",
    "@artstesh/postboy": "3.1.2",
    "@ngbracket/ngx-layout": "17.0.1",
    "@ngx-translate/core": "15.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@schematics/angular": "17.3.9",
    "@swimlane/ngx-datatable": "20.1.0",
    "angular-auth-oidc-client": "18.0.2",
    "cronstrue": "2.53.0",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "ngx-cookie-service": "17.1.0",
    "ngx-translate-cache": "17.0.0",
    "ol": "6.15.1",
    "rxjs": "7.8.1",
    "tslib": "2.3.0",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "17.0.2",
    "@angular-devkit/build-angular": "17.3.8",
    "@angular-eslint/builder": "19.0.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/schematics": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/cli": "17.3.11",
    "@angular/compiler-cli": "17.3.12",
    "@artstesh/forger": "2.0.0",
    "@artstesh/it-should": "2.0.1",
    "@artstesh/postboy-testing": "1.1.1",
    "@compodoc/compodoc": "1.1.26",
    "@types/jasmine": "3.8.1",
    "@types/lodash": "^4.17.20",
    "@types/node": "12.11.1",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "@vendure/ngx-translate-extract": "8.3.0",
    "angular-eslint": "19.0.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "html-to-image": "1.11.3",
    "husky": "8.0.3",
    "jasmine-core": "3.8.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "ng-mocks": "14.12.1",
    "ng-openapi-gen": "0.51.0",
    "pre-commit": "1.2.2",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-tailwindcss": "0.6.6",
    "pretty-quick": "4.0.0",
    "ts-mockito": "2.6.1",
    "typescript": "5.2.2"
  }
}
